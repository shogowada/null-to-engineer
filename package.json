{
  "name": "null-to-web-engineer",
  "version": "1.2.1",
  "description": "",
  "main": "/src/server/index.ts",
  "scripts": {
    "format": "npm run generate-code && (pretty-quick || echo \"Ignoring the format failure\")",
    "check-type": "tsc --noEmit",
    "generate-code": "ts-node --transpile-only ./src/generate-code.ts",
    "build": "webpack-cli",
    "build-base": "docker build --tag null-to-web-engineer:base --file Dockerfile-base .",
    "_start": "ts-node ./src/server/index.ts",
    "start": "npm run format && npm run build-base && docker-compose up --build -d",
    "unit-test-client": "mocha --require ts-node/register \"./src/client/**/*.spec.ts\"",
    "unit-test-server": "mocha --require ts-node/register \"./src/server/**/*.spec.ts\"",
    "e2e-test": "npm run e2e-test-api && npm run e2e-test-gui",
    "e2e-test-api": "cucumber-js --profile api",
    "e2e-test-gui": "cucumber-js --profile gui",
    "e2e-test-wip": "npm run e2e-test-api-wip && npm run e2e-test-gui-api",
    "e2e-test-api-wip": "cucumber-js --profile apiWIP",
    "e2e-test-gui-wip": "cucumber-js --profile guiWIP",
    "test": "npm run format && npm run build-base && docker-compose --file docker-compose.yml --file docker-compose.test.yml up --build --abort-on-container-exit"
  },
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.12",
    "@types/helmet": "^4.0.0",
    "@types/marked": "^2.0.3",
    "@types/node": "^15.12.4",
    "@types/node-fetch": "^2.5.10",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "highlight.js": "^11.0.1",
    "http-status-codes": "^2.1.4",
    "json-rpc-2.0": "^0.2.16",
    "marked": "^2.1.3",
    "node-fetch": "^2.6.1",
    "re-reselect": "^4.0.0",
    "reselect": "^4.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^7.3.0",
    "@types/chai": "^4.2.19",
    "@types/history": "^4.7.8",
    "@types/mocha": "^8.2.3",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/react-router": "^5.1.15",
    "@types/react-router-dom": "^5.1.7",
    "@types/selenium-webdriver": "^3.0.17",
    "autoprefixer": "^10.2.6",
    "chai": "^4.3.4",
    "chromedriver": "^91.0.1",
    "connected-react-router": "^6.9.1",
    "css-loader": "^5.2.6",
    "history": "^4.10.1",
    "html-webpack-plugin": "^5.3.1",
    "mocha": "^8.4.0",
    "postcss-loader": "^6.1.0",
    "prettier": "^2.3.1",
    "pretty-quick": "^3.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.3.0",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "selenium-webdriver": "^3.6.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.2.3",
    "webpack": "^5.40.0",
    "webpack-cli": "^4.7.2"
  }
}
